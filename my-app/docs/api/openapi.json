  "components": {
    "securitySchemes": {
      "bearer": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "LoginDto": {
        "type": "object",
        "properties": {
          "loginId": {
            "type": "string",
            "description": "사용자 로그인 ID",
            "example": "user123"
          },
          "password": {
            "type": "string",
            "description": "사용자 비밀번호",
            "example": "password123"
          }
        },
        "required": [
          "loginId",
          "password"
        ]
      },
      "LoginResponseDto": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string",
            "description": "액세스 토큰"
          },
          "refreshToken": {
            "type": "string",
            "description": "리프레시 토큰"
          }
        },
        "required": [
          "accessToken",
          "refreshToken"
        ]
      },
      "SignupRequestDto": {
        "type": "object",
        "properties": {
          "loginId": {
            "type": "string",
            "description": "로그인 ID",
            "example": "user123"
          },
          "pw": {
            "type": "string",
            "description": "비밀번호",
            "minLength": 8,
            "example": "password123"
          },
          "realName": {
            "type": "string",
            "description": "실명",
            "example": "박재인"
          },
          "phoneNumber": {
            "type": "string",
            "description": "전화번호",
            "example": "01012345678"
          },
          "profileImageUrl": {
            "type": "string",
            "description": "프로필 이미지 Url (없을 시 기본 이미지 url)",
            "example": "https://example.com/profile.jpg"
          },
          "affiliation": {
            "type": "string",
            "description": "소속",
            "example": "○○병원 응급의학과"
          },
          "termsIdList": {
            "type": "array",
            "description": "동의한 약관 ID 리스트",
            "items": {
              "type": "number"
            },
            "example": [
              1,
              2
            ]
          }
        },
        "required": [
          "loginId",
          "pw",
          "realName",
          "termsIdList"
        ]
      },
      "SignupResponseDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "number",
            "description": "생성된 유저 ID",
            "example": 1
          }
        },
        "required": [
          "userId"
        ]
      },
      "CreateOrganizationPostRequestDto": {
        "type": "object",
        "properties": {
          "content": {
            "type": "string",
            "description": "게시글 내용"
          },
          "fileUrl": {
            "type": "string",
            "description": "첨부파일 URL"
          }
        },
        "required": [
          "content"
        ]
      },
      "CreateOrganizationPostResponseDto": {
        "type": "object",
        "properties": {
          "organizationPostId": {
            "type": "number",
            "description": "생성된 게시글 ID"
          }
        },
        "required": [
          "organizationPostId"
        ]
      },
      "OrganizationPostDto": {
        "type": "object",
        "properties": {
          "organizationPostId": {
            "type": "number"
          },
          "content": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "fileUrl": {
            "type": [
              "string",
              "null"
            ]
          }
        },
        "required": [
          "organizationPostId",
          "content",
          "createdAt",
          "fileUrl"
        ]
      },
      "GetOrganizationPostListResponseDto": {
        "type": "object",
        "properties": {
          "postList": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OrganizationPostDto"
            }
          },
          "totalCount": {
            "type": "number"
          }
        },
        "required": [
          "postList",
          "totalCount"
        ]
      },
      "UpdateOrganizationPostRequestDto": {
        "type": "object",
        "properties": {
          "content": {
            "type": "string"
          },
          "fileUrl": {
            "type": "string"
          }
        },
        "required": [
          "content"
        ]
      },
      "UpdateOrganizationPostResponseDto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        },
        "required": [
          "message"
        ]
      },
      "DeleteOrganizationPostResponseDto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        },
        "required": [
          "message"
        ]
      },
      "GetScenarioListByLicenseResponseDto": {
        "type": "object",
        "properties": {
          "scenarioList": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ScenarioItemDto"
            }
          },
          "totalCount": {
            "type": "number"
          }
        },
        "required": [
          "scenarioList",
          "totalCount"
        ]
      },
      "ScenarioItemDto": {
        "type": "object",
        "properties": {
          "scenarioId": {
            "type": "number"
          },
          "title": {
            "type": "string"
          },
          "patientList": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PatientInfoDto"
            }
          },
          "mapItemName": {
            "type": "string"
          },
          "expectedPlayTimeSeconds": {
            "type": "number"
          },
          "playCount": {
            "type": "number"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": [
          "scenarioId",
          "title",
          "patientList",
          "mapItemName",
          "expectedPlayTimeSeconds",
          "playCount",
          "updatedAt"
        ]
      },
      "PatientInfoDto": {
        "type": "object",
        "properties": {
          "age": {
            "type": "number"
          },
          "gender": {
            "type": "string"
          }
        },
        "required": [
          "age",
          "gender"
        ]
      }
    }
  }
